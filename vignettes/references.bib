  journal = {Automation in Construction},
  keywords = {Architectural design,Artificial intelligence,Artificial intelligence in architecture,Building design,Environmental design,Generative systems,Genetic algorithms,Genetic algorithms in architecture,Low–energy design,Optimization in architecture},
