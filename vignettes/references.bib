@article{Rhodes2015,
  title = {Using {{BEopt}} ({{EnergyPlus}}) with Energy Audits and Surveys to Predict Actual Residential Energy Usage},
  author = {Rhodes, Joshua D. and Gorman, William H. and Upshaw, Charles R. and Webber, Michael E.},
